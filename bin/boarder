#!/usr/bin/env node
const inquirer = require('inquirer');

const argv = require('yargs')
  .usage('Usage: $0 init [Project Name]')
  .command(
    'init',
    'init base on webpack demo',
    yargs => {

    },
    argv => {
      const projectName = argv._[1];
    }
  )
  .example('$0 init webpack-demo')
  .help('h')
  .alias('h', 'help')
  .alias('-v', '--version')
  .argv;